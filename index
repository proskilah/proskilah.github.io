												<td class="column column-1" width="100%" style="mso-table-lspace: 0pt; mso-table-rspace: 0pt; font-weight: 400; text-align: left; vertical-align: top; border-top: 0px; border-right: 0px; border-bottom: 0px; border-left: 0px;">
													<div class="spacer_block block-1" style="height:25px;line-height:25px;font-size:1px;">&#8202;</div>
												</td>
											</tr>
										</tbody>
									</table>
								</td>
							</tr>
						</tbody>
					</table>
				</td>
			</tr>
		</tbody>
	</table><!-- End -->
</body>

</html>` // Aquí irá el código de la nueva plantilla
        };

        function toggleHousing() {
            const housingCheckbox = document.getElementById('housing');
            const housingFieldContainer = document.getElementById('housingFieldContainer');
            const nombreSelect = document.getElementById('nombre');

            if (housingCheckbox.checked) {
                housingFieldContainer.style.display = 'block';
                nombreSelect.disabled = true;
            } else {
                housingFieldContainer.style.display = 'none';
                nombreSelect.disabled = false;
            }
        }

        function toggleNuevaPlantilla() {
            const nuevaPlantillaCheckbox = document.getElementById('nuevaPlantilla');
            const idiomaSelect = document.getElementById('idioma');
            const direccionCheckbox = document.getElementById('direccion');
            const oficinaSelect = document.getElementById('telefonoLugar');
            const nombreSelect = document.getElementById('nombre');

            if (nuevaPlantillaCheckbox.checked) {
                idiomaSelect.disabled = true;
                direccionCheckbox.disabled = true;
                oficinaSelect.disabled = true;

                nombreSelect.innerHTML = '';
                const todasLasOpciones = [
                    'Secretaría Técnica', 'Agencia Organizadora',
                    'Technical Secretariat', 'Organizing Agency', 'Housing Agency',
                    'Agència Organitzadora', 'Secretaria Tècnica'
                ];

                todasLasOpciones.forEach(function(opcion) {
                    const optionElement = document.createElement('option');
                    optionElement.value = opcion;
                    optionElement.textContent = opcion;
                    nombreSelect.appendChild(optionElement);
                });
            } else {
                idiomaSelect.disabled = false;
                direccionCheckbox.disabled = false;
                oficinaSelect.disabled = false;
                idiomaSelect.dispatchEvent(new Event('change'));
            }
        }

        function generarCodigo() {
            const nuevaPlantillaChecked = document.getElementById('nuevaPlantilla').checked;
            const idioma = document.getElementById('idioma').value;
            const incluirDireccion = document.getElementById('direccion').checked;
            const housingChecked = document.getElementById('housing').checked;
            const nombreCongreso = document.getElementById('nombreCongreso').value;
            const nombreSeleccionado = document.getElementById('nombre').value;
            const email = document.getElementById('email').value;
            const web = document.getElementById('web').value;
            const telefonoTipo = document.getElementById('telefonoTipo').value;
            const telefonoLugar = document.getElementById('telefonoLugar').value;

            let key = nuevaPlantillaChecked ? 'nueva' : `${idioma}_${incluirDireccion ? 'con' : 'sin'}`;
            let codigo = codigos[key];

            if (!codigo) {
                document.getElementById('codigoGenerado').value = "No se encontró un código correspondiente.";
                return;
            }

            const prefijo = telefonoTipo === 'internacional' ? '+34 ' : '';
            const numero = telefonoLugar === 'barcelona' ? '938 823 878' : '954 224 095';
            codigo = codigo.replaceAll('número', `${prefijo}${numero}`);

            if (telefonoLugar === 'sevilla') {
                codigo = codigo.replace('Pl. Europa, 17-19 1ª planta - 08908', 'C/ Gonzalo Bilbao 23-25, 2ª planta, mód 9-10');
                codigo = codigo.replace('L’Hospitalet de Llobregat, Barcelona', '41003 Sevilla, España');
            }

            if (housingChecked && nombreCongreso.trim() !== '') {
                const housingName = `${nombreCongreso} | Official Housing Agency`;
                codigo = codigo.replace('nombre', housingName);
            } else {
                codigo = codigo.replace('nombre', nombreSeleccionado);
            }

            if (email) {
                codigo = codigo.replace(/coche/g, email);
            } else {
                codigo = codigo.replace(/coche/g, '');
            }

            if (web) {
                codigo = codigo.replace(/moto/g, web);
            } else {
                codigo = codigo.replace(/moto/g, '');
            }

            document.getElementById('codigoGenerado').value = codigo;
        }

        function copiarCodigo() {
            const codigo = document.getElementById('codigoGenerado');
            codigo.select();
            document.execCommand('copy');
            alert("Código copiado al portapapeles.");
        }

        function openModal() {
            const codigo = document.getElementById('codigoGenerado').value;
            const modal = document.getElementById('myModal');
            const vistaPrevia = document.getElementById('vistaPrevia');
            vistaPrevia.src = "data:text/html;charset=utf-8," + encodeURIComponent(codigo);
            modal.style.display = "block";
        }

        function closeModal() {
            const modal = document.getElementById('myModal');
            modal.style.display = "none";
        }

        document.getElementById('idioma').addEventListener('change', function() {
            const idioma = this.value;
            const nombreSelect = document.getElementById('nombre');
            nombreSelect.innerHTML = '';

            if (idioma === 'es') {
                const opciones = ['Secretaría Técnica', 'Agencia Organizadora'];
                opciones.forEach(function(opcion) {
                    const optionElement = document.createElement('option');
                    optionElement.value = opcion;
                    optionElement.textContent = opcion;
                    nombreSelect.appendChild(optionElement);
                });
            } else if (idioma === 'en') {
                const opciones = ['Technical Secretariat', 'Organizing Agency', 'Housing Agency'];
                opciones.forEach(function(opcion) {
                    const optionElement = document.createElement('option');
                    optionElement.value = opcion;
                    optionElement.textContent = opcion;
                    nombreSelect.appendChild(optionElement);
                });
            } else if (idioma === 'ca') {
                const opciones = ['Agència Organitzadora', 'Secretaria Tècnica'];
                opciones.forEach(function(opcion) {
                    const optionElement = document.createElement('option');
                    optionElement.value = opcion;
                    optionElement.textContent = opcion;
                    nombreSelect.appendChild(optionElement);
                });
            }
        });

        document.getElementById('idioma').dispatchEvent(new Event('change'));
    </script>
</body>

</html>
